<p><strong style="mso-bidi-font-weight: normal"><span style="text-decoration: underline;">ITEM</span><span style="mso-spacerun: yes">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; </span><span style="text-decoration: underline;">DESCRIPTION</span><span style="mso-spacerun: yes">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; </span><span style="mso-spacerun: yes">&nbsp;&nbsp;</span><span style="mso-spacerun: yes">&nbsp;&nbsp;</span><span style="text-decoration: underline;">U/I</span><span style="mso-spacerun: yes">&nbsp;&nbsp;&nbsp;&nbsp; </span><span style="text-decoration: underline;">QTY</span> <span style="mso-spacerun: yes">&nbsp;&nbsp;&nbsp;&nbsp;</span><span style="text-decoration: underline;">UNIT PRICE</span><span style="mso-spacerun: yes">&nbsp;&nbsp;&nbsp;&nbsp; </span><span style="text-decoration: underline;">TOTAL PRICE</span></strong></p>
