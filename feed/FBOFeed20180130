<ol style="MARGIN-TOP: 0in" type="a">
<li>&nbsp; 
<ul style="MARGIN-TOP: 0in" type="disc">
<li style="MARGIN-BOTTOM: 0pt; LINE-HEIGHT: normal; MARGIN-RIGHT: 9pt; mso-pagination: none; mso-add-space: auto; mso-list: l0 level2 lfo1"><span style="FONT-SIZE: 12pt; LETTER-SPACING: -0.1pt; mso-bidi-font-family: Calibri">C</span><span style="FONT-SIZE: 12pt; LETTER-SPACING: 0.1pt; mso-bidi-font-family: Calibri">a</span><span style="FONT-SIZE: 12pt; LETTER-SPACING: -0.05pt; mso-bidi-font-family: Calibri">b</span><span style="FONT-SIZE: 12pt; mso-bidi-font-family: Calibri">le <span style="LETTER-SPACING: -0.1pt">j</span><span style="LETTER-SPACING: 0.1pt">a</span>c<span style="LETTER-SPACING: -0.1pt">k</span>et<span style="LETTER-SPACING: -0.05pt"> </span><span style="LETTER-SPACING: -0.1pt">s</span><span style="LETTER-SPACING: -0.05pt">h</span><span style="LETTER-SPACING: 0.1pt">a</span>ll n<span style="LETTER-SPACING: -0.05pt">o</span>t<span style="LETTER-SPACING: -0.1pt"> s</span><span style="LETTER-SPACING: -0.05pt">n</span><span style="LETTER-SPACING: 0.1pt">a</span>g<span style="LETTER-SPACING: 0.1pt"> or </span>f<span style="LETTER-SPACING: -0.1pt">r</span><span style="LETTER-SPACING: 0.1pt">a</span>y.</span></li>
</ul>
</li>
</ol>
<p style="MARGIN: 0in 9pt 0pt 1in; LINE-HEIGHT: normal; TEXT-INDENT: -0.25in; mso-pagination: none; mso-add-space: auto; tab-stops: 59.0pt; mso-list: l0 level2 lfo1"><span style="FONT-SIZE: 12pt; FONT-FAMILY: Symbol; mso-bidi-font-family: Symbol; mso-fareast-font-family: Symbol"><span>&bull;&middot;</span></span><span style="FONT-SIZE: 12pt; LETTER-SPACING: -0.1pt; mso-bidi-font-family: Calibri">C</span><span style="FONT-SIZE: 12pt; LETTER-SPACING: 0.1pt; mso-bidi-font-family: Calibri">a</span><span style="FONT-SIZE: 12pt; LETTER-SPACING: -0.05pt; mso-bidi-font-family: Calibri">b</span><span style="FONT-SIZE: 12pt; mso-bidi-font-family: Calibri">le <span style="LETTER-SPACING: -0.1pt">j</span><span style="LETTER-SPACING: 0.1pt">a</span>c<span style="LETTER-SPACING: -0.1pt">k</span>et<span style="LETTER-SPACING: -0.05pt"> (convoluted tubing) </span><span style="LETTER-SPACING: -0.1pt">s</span><span style="LETTER-SPACING: -0.05pt">h</span><span style="LETTER-SPACING: 0.1pt">a</span>ll be<span style="LETTER-SPACING: -0.1pt"> </span><span style="LETTER-SPACING: 0.1pt">a</span><span style="LETTER-SPACING: -0.05pt">b</span><span style="LETTER-SPACING: -0.15pt">r</span><span style="LETTER-SPACING: 0.1pt">a</span><span style="LETTER-SPACING: -0.1pt">s</span>i<span style="LETTER-SPACING: -0.1pt">o</span><span style="LETTER-SPACING: -0.05pt">n</span>, che<span style="LETTER-SPACING: -0.05pt">m</span>ic<span style="LETTER-SPACING: 0.1pt">a</span>l<span style="LETTER-SPACING: -0.1pt"> </span><span style="LETTER-SPACING: 0.1pt">a</span><span style="LETTER-SPACING: -0.05pt">n</span>d<span style="LETTER-SPACING: -0.05pt"> </span>h<span style="LETTER-SPACING: -0.1pt">e</span><span style="LETTER-SPACING: 0.1pt">a</span>t<span style="LETTER-SPACING: -0.05pt"> </span><span style="LETTER-SPACING: -0.15pt">r</span>e<span style="LETTER-SPACING: -0.1pt">s</span><span style="LETTER-SPACING: 0.1pt">i</span>s<span style="LETTER-SPACING: -0.1pt">t</span><span style="LETTER-SPACING: 0.1pt">a</