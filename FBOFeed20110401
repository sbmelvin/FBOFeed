<colgroup span="1"><col style="width: 135pt; mso-width-source: userset; mso-width-alt: 6582;" span="1" width="180"></col><col style="width: 317pt; mso-width-source: userset; mso-width-alt: 15433;" span="1" width="422"></col><col style="width: 305pt; mso-width-source: userset; mso-width-alt: 14848;" span="1" width="406"></col><col style="width: 271pt; mso-width-source: userset; mso-width-alt: 13202;" span="1" width="361"></col><col style="width: 292pt; mso-width-source: userset; mso-width-alt: 14226;" span="1" width="389"></col></colgroup>
