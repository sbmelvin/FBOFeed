<table style="width: 491.4pt; border-collapse: collapse; margin-left: 6.75pt; margin-right: 6.75pt; mso-yfti-tbllook: 1184; mso-table-lspace: 9.0pt; mso-table-rspace: 9.0pt; mso-table-anchor-vertical: paragraph; mso-table-anchor-horizontal: column; mso-table-left: left; mso-padding-alt: 0in 0in 0in 0in;" border="0" cellspacing="0" cellpadding="0" width="655" align="left">
<tbody>
<tr style="height: 13.5pt; mso-yfti-irow: 0; mso-yfti-firstrow: yes;">
<td style="padding-bottom: 0in; padding-left: 5.4pt; width: 45.95pt; padding-right: 5.4pt; background: #ff99cc; height: 13.5pt; padding-top: 0in; border: windowtext 1pt solid;" width="61">
<p style="TEXT-ALIGN: center; LINE-HEIGHT: normal; MARGIN-BOTTOM: 0pt; mso-element: frame; mso-element-frame-hspace: 9.0pt; mso-element-wrap: around; mso-element-anchor-vertical: paragraph; mso-element-anchor-horizontal: column; mso-height-rule: exactly" align="center"><span style="FONT-FAMILY: 'Arial', 'sans-serif'; COLOR: black; FONT-SIZE: 8pt; mso-fareast-font-family: 'Times New Roman'">SIN NUMBER</span></p>
</td>
<td style="border-bottom: windowtext 1pt solid; border-left: medium none; padding-bottom: 0in; padding-left: 5.4pt; width: 103.45pt; padding-right: 5.4pt; background: #ff99cc; height: 13.5pt; border-top: windowtext 1pt solid; border-right: windowtext 1pt solid; padding-top: 0in;" width="138">
<p style="TEXT-ALIGN: center; LINE-HEIGHT: normal; MARGIN-BOTTOM: 0pt; mso-element: frame; mso-element-frame-hspace: 9.0pt; mso-element-wrap: around; mso-element-anchor-vertical: paragraph; mso-element-anchor-horizontal: column; mso-height-rule: exactly" align="center"><span style="FONT-FAMILY: 'Arial', 'sans-serif'; COLOR: black; FONT-SIZE: 8pt; mso-fareast-font-family: 'Times New Roman'">SERVICE</span></p>
</td>
<td style="border-bottom: windowtext 1pt solid; border-left: medium none; padding-bottom: 0in; padding-left: 5.4pt; width: 45pt; padding-right: 5.4pt; background: #ff99cc; height: 13.5pt; border-top: windowtext 1pt solid; border-right: windowtext 1pt solid; padding-top: 0in;" width="60">
